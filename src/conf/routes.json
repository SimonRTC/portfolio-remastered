{
    "default": [
        {
            "method": "GET|POST",
            "pattern": "/",
            "service": "Portfolio::Index"
        }
    ],
    "help-center": [
        {
            "method": "GET",
            "pattern": "/",
            "service": "HelpCenter::Projects"
        },
        {
            "method": "GET",
            "pattern": "/{project-name}",
            "service": "HelpCenter::Project"
        },
        {
            "method": "GET",
            "pattern": "/{project-name}/documentation",
            "service": "HelpCenter::Documentation"
        }
    ]
}